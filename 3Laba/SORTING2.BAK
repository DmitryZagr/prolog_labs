domains
   tdata = data(string, integer)
   tdatalist = tdata*
   
 predicates
   nondeterm women(string, integer) 
   nondeterm man(string, integer)
   
   nondeterm getdata_w(tdata)
   getdatalist_w(tdatalist)
   
   nondeterm getdata_m(tdata)
   getdatalist_m(tdatalist)
   
   nondeterm printlist(tdatalist)
   
 clauses
   women("Galina", 23).
   women("Katerina", 31).
   women("Dasha", 21).
   women("Nelly", 28).
   women("Aleksandra", 18).
   
   man("Vladimir", 24).
   man("Sergey", 21).
   man("Petr", 31).
   man("Trofim", 27).
   man("Aleksey", 18).
   

   
   getdata_w(data(Name, Age)) :- women(Name, Age), Age > 0.
   getdatalist_w(OrderList) :- findall(Order, getdata_w(Order), OrderList).
   
   getdata_m(data(Name, Age)) :- man(Name, Age), Age > 0.
   getdatalist_m(OrderList) :- findall(Order, getdata_m(Order), OrderList).
   
   printlist([]).
   printlist([data(Product,Qty)|Tail]) :- write(Product,"\t",Qty), nl, printlist(Tail).
   
   
 goal
   getdatalist_w(WList), 
   printlist(WList),  
   write("*******************"), nl,
   getdatalist_m(MList), 
   printlist(MList), 
   write("*******************"), nl,
   fail.